{
  "name": "e-moms",
  "version": 2,
  "builds": [{ "src": "src/index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/(.*)", "dest": "src/index.js" }],
  "env": {
    "DB_USER": "@emoms_db_user",
    "DB_PASSWORD": "@emoms_db_password",
    "DB_HOST": "@emoms_db_host",
    "DB_NAME": "@emoms_db_name"
  }
}